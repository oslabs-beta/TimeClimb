{
  "name": "time-climb",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "test": "vitest",
    "lint": "eslint .",
    "preview": "vite preview",
    "build:server": "tsc -b tsconfig.server.json",
    "start": "node dist/server/server.js",
    "dev:server": "tsx watch server/server.ts",
    "setup:database": "npx tsx ./database/setupDatabase",
    "migrate:make": "npx tsx ./node_modules/.bin/knex migrate:make",
    "migrate:latest": "npx tsx ./node_modules/.bin/knex migrate:latest",
    "migrate:rollback": "npx tsx ./node_modules/.bin/knex migrate:rollback",
    "seed:make": "npx tsx ./node_modules/.bin/knex seed:make",
    "seed:run": "npx tsx ./node_modules/.bin/knex seed:run"
  },
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.675.0",
    "@aws-sdk/client-sfn": "^3.670.0",
    "@aws-sdk/credential-providers": "^3.670.0",
    "@dagrejs/dagre": "^1.1.4",
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.1.5",
    "@reduxjs/toolkit": "^2.2.8",
    "@xyflow/react": "^12.3.1",
    "bottleneck": "^2.19.5",
    "chart.js": "^4.4.5",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "knex": "^3.1.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.46",
    "node-cron": "^3.0.3",
    "pg": "^8.13.0",
    "plotly.js-dist": "^2.35.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.27.0",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "ts-node-dev": "^2.0.0",
    "vite-express": "^0.19.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.11.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.9.0",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.8",
    "vitest": "^2.1.2"
  }
}
