    "eslint-plugin-react-refresh": "^0.4.12",
    "globals": "^15.9.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.8"
